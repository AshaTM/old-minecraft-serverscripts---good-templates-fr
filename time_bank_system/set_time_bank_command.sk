command /set_time_bank:
    permission: command.time_bank
    trigger:
        set {_loc} to center of target block of player
        set {_x} to x-coordinate of {_loc}
        set {_y} to y-coordinate of {_loc}
        set {_z} to z-coordinate of {_loc}
        set {_world} to world of {_loc}
        setTimeBank({_x}, {_y}, {_z}, {_world})